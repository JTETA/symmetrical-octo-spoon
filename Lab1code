    a = 0.97;
    b = 5.61;
    c = 0.31;
    x = -2.5;
    k = 2;
    y = log (k * x) / sin (7) - sqrt(abs(x - power (a, 2))) - (10000 * a - b) / cos (k * x) + power(x-power(a,2), 1 / 3) - power(c, 3) * x
    m = [a b c x k y]
    [useless, maximum] =  max(m);
    [useless, minimum] =  min(m);
    m(maximum) = maximum;
    m(minimum) = minimum;
    m = sort(m)
