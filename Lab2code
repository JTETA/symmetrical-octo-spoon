v = [input("v1 ") input("v2 ")];
w = [input("w1 ")
    input("w2 ")];
m = [input("m11 ") input("m12 ")
    input("m21 ") input("m22 ")];
m0 = zeros(2,2);
m1 = ones(2,2);
mr = rand(2,2);%также можно использовать magic(2)
me = diag([1 1]);
M = w*v + mr * me
[height, length] = size(M)
maximum = max(max(M))
minimum = min(min(M))
summa = sum(sum(M))
production = prod(prod(M))

